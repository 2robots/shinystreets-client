<?xml version='1.0' encoding='utf-8'?>
<widget id="com.Shinystreets" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Shinystreets</name>
    <description>Community based local improvements.</description>
    <author email="franz@2robots.at" href="http://Shinystreets.com">Franz Wilding</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />

    <feature name="Console">
      <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
      <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="PushPlugin">
      <param name="ios-package" value="PushPlugin"/>
    </feature>
    <feature name="File">
      <param name="ios-package" value="CDVFile" />
    </feature>
    <feature name="FileTransfer">
      <param name="ios-package" value="CDVFileTransfer" />
    </feature>
</widget>
